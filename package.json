{
  "name": "parvus-ui",
  "version": "0.1.3",
  "description": "A small footprint shareable UI component library for React and Preact",
  "repository": "github:rnglnd/parvus-ui",
  "author": "Steven Ringland <steven.ringland@gmail.com>",
  "license": "MIT",
  "keywords": [
    "react",
    "preact",
    "component",
    "library",
    "ui",
    "ux",
    "design"
  ],
  "bundlesize": [
    {
      "path": "./lib/index.js",
      "maxSize": "30 kB"
    }
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "unpkg": "dist/parvus-ui.min.js",
  "scripts": {
    "bundlesize": "bundlesize",
    "flow": "flow",
    "start": "webpack-dev-server --mode development",
    "test": "jest",
    "build": "rm -rf lib && ./node_modules/.bin/webpack --mode=production && BABEL_ENV=es babel src -d es",
    "precommit": "exit 0",
    "postinstall": "flow-typed install && flow-typed create-stub cxs@x && flow-typed create-stub create-react-context@x",
    "prepublish": "rm -rf lib && rm -rf es && npm run build",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "lint-staged": {
    "*.{js,jsx,json}": [
      "prettier-eslint --write",
      "git add"
    ]
  },
  "peerDependencies": {
    "react": "^0.14.6 || 15.x.x || 16.x.x",
    "react-dom": "^0.14.6 || 15.x.x || 16.x.x",
    "prop-types": "15.x.x"
  },
  "dependencies": {
    "create-react-context": "^0.2.2",
    "cxs": "^6.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "8.2.3",
    "babel-jest": "22.4.4",
    "babel-loader": "7.1.5",
    "babel-plugin-add-module-exports": "0.2.1",
    "babel-plugin-flow-react-proptypes": "^24.1.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "bundlesize": "^0.17.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.19.1",
    "eslint-config-unobtrusive": "1.2.2",
    "eslint-loader": "2.0.0",
    "eslint-plugin-flowtype": "2.49.3",
    "eslint-plugin-import": "2.10.0",
    "eslint-plugin-react": "7.8.2",
    "flow-bin": "^0.77.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "0.14.3",
    "jest": "22.4.4",
    "lint-staged": "^7.2.0",
    "path": "^0.12.7",
    "prettier": "1.12.1",
    "prettier-eslint": "8.8.1",
    "prettier-eslint-cli": "4.7.1",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-styleguidist": "^7.3.0",
    "webpack": "^4.16.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "./node_modules/babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/))\\.js?$",
    "moduleFileExtensions": [
      "js",
      "json"
    ]
  }
}
