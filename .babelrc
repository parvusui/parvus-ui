{
  "presets": [
    [
      "env",
      {
        "modules": false,
        "targets": {
          "browsers": ["last 2 versions", "safari > 8", "not ie < 11"]
        }
      }
    ],
    "react",
    "stage-2",
    "flow"
  ],
  "plugins": ["external-helpers"],
  "env": {
    "development": {
      "plugins": [
        [
          "flow-react-proptypes",
          {
            "ignoreNodeModules": true
          }
        ]
      ]
    }
  }
}
